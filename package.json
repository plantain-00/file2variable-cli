{
  "name": "file2variable-cli",
  "version": "1.4.0",
  "description": "A CLI tool to read file content and assign it to a variable",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc -p src",
    "lint": "tslint \"src/*.ts\"",
    "clean": "rimraf dist",
    "demo": "node dist/index.js demo/*.html demo/*.json demo/*.proto -o demo/variables.js --html-minify --json --protobuf --base demo",
    "test": "tsc -p spec && jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/file2variable-cli.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/file2variable-cli/issues"
  },
  "homepage": "https://github.com/plantain-00/file2variable-cli#readme",
  "devDependencies": {
    "@types/camelcase": "0.0.30",
    "@types/glob": "5.0.30",
    "@types/html-minifier": "1.1.30",
    "@types/jasmine": "2.5.53",
    "@types/minimist": "1.2.0",
    "jasmine": "2.6.0",
    "rimraf": "2.6.1",
    "tslint": "5.4.3",
    "typescript": "2.4.1"
  },
  "dependencies": {
    "camelcase": "4.1.0",
    "glob": "7.1.2",
    "html-minifier": "3.5.2",
    "lodash.flatten": "4.4.0",
    "lodash.uniq": "4.5.0",
    "minimist": "1.2.0",
    "protobufjs": "6.8.0"
  },
  "bin": {
    "file2variable-cli": "./bin/file2variable-cli"
  },
  "engines": {
    "node": ">=4"
  }
}
